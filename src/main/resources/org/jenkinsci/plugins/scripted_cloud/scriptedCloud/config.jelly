<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" 
	xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">
    
    <f:entry title="${%Name to use for this scripted Cloud}" 
    			help="/plugin/scripted-cloud-plugin/vsDescription.html">
        <f:textbox field="vsDescription" />
    </f:entry>
    
    <f:entry title="${%Batch or a shell script files to start machine}"
    		help="/plugin/scripted-cloud-plugin/slave-startscript.html">
        <f:textbox field="startScriptFile"></f:textbox>
    </f:entry>
    
    <f:entry title="${%Batch or a shell script files to stop machine}"
    	help="/plugin/scripted-cloud-plugin/slave-stopscript.html">
        <f:textbox field="stopScriptFile"></f:textbox>
    </f:entry>
    
</j:jelly>
